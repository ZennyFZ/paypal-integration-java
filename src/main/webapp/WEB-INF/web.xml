<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>cancel</servlet-name>
        <jsp-file>/cancel.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>cancel</servlet-name>
        <url-pattern>/cancel</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ReviewPayment</servlet-name>
        <jsp-file>/reviewpayment.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>ReviewPayment</servlet-name>
        <url-pattern>/reviewpayment</url-pattern>
    </servlet-mapping>
    <welcome-file-list>
        <welcome-file>checkout.jsp</welcome-file>
    </welcome-file-list>
</web-app>
